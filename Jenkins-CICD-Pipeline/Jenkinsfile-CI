@Library('github.com/ranjitaws2020/dotnet-shared-library@pipeline') _

pipeline {
    agent any
      environment {
       sonarlogin = credentials('sonarqube-token')
       }
      tools {        
        jdk 'java_jdk'
      }
    stages {
        stage('Dependencies') {
            steps{
		 RestoreDependencies()
            }
        }
        stage('Build'){
            steps {
		  BuildApp()
            }
        }
         stage('SonarQube Analysis') {
            steps{
	    	SonarApp()
	    }
        }
        stage('Publish') {
            steps{
		PublishApp()
            }
        }       
        stage('Store war to Nexus') {
            steps{
	    	StoreToNexus()
            }
        }      
    }
}
