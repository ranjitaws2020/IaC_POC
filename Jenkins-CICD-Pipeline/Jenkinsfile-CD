@Library('github.com/ranjitaws2020/dotnet-shared-library@pipeline') _

pipeline {
    agent any
      tools {       
        jdk 'java_jdk'
      }
    stages {
    	stage('Pull the file off Nexus') {
            steps{
                DownloadFromNexus()                
            }
         }
        stage('Host app to IIS') {
            steps{
                 DeployToIIS()
            }
        }
    }
}
